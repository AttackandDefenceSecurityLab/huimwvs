{
  "manifest_version": 2,
  "name": "huimwvs助手",
  "version": "1.0.0",
  "description": "huimwvs扫描器获取流量进行扫描的入口",
  "icons": 
  {
    "16": "img/16icon.png",
    "48": "img/48icon.png",
    "128": "img/128icon.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/16icon.png",
      "48": "img/48icon.png"
    },
    "default_title": "huimwvs-assistant",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "page":"background.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "webNavigation",
    "debugger",
    "http://*/*",
    "https://*/*"
  ]
}